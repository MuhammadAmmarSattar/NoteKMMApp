CREATE TABLE noteEntity(
    id INTEGER NOT NULL PRIMARY KEY AUTOINCREMENT,
    title TEXT NOT NULL,
    date TEXT NOT NULL,
    image TEXT NOT NULL,
    content TEXT NOT NULL,
    colorHex INTEGER NOT NULL
);

getAllNotes:
SELECT *
FROM noteEntity;


getNoteById:
SELECT *
FROM noteEntity
WHERE id = ?;

insertNote:
INSERT OR REPLACE
INTO noteEntity(
    id,
    title,
    date,
    image,
    content,
    colorHex
) VALUES(?,?,?, ?, ?, ?);

deleteNoteById:
DELETE FROM noteEntity
WHERE id = ?;